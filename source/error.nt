import {
  this.pos {pos},
}

data observation {
| Rune(rune)
| Text(text)
| End-Of-Input
}

data error {
| Error(
    found: observation,
    expected: list(observation),
    position: pos,
  )
}

define unexpected-end-of-input(p: pos): error {
  Error of {found = End-Of-Input, expected = [], position = p}
}
